{
  "name": "@qckfx/agent",
  "version": "0.1.1",
  "type": "module",
  "description": "qckfx AI Agent SDK",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./tools": {
      "types": "./dist/esm/src/tools/index.d.ts",
      "import": "./dist/esm/src/tools/index.js",
      "require": "./dist/cjs/src/tools/index.js"
    },
    "./providers": {
      "types": "./dist/esm/src/providers/index.d.ts",
      "import": "./dist/esm/src/providers/index.js",
      "require": "./dist/cjs/src/providers/index.js"
    },
    "./internals": {
      "types": "./dist/esm/src/internals/index.d.ts",
      "import": "./dist/esm/src/internals/index.js",
      "require": "./dist/cjs/src/internals/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json && node -e \"require('fs').writeFileSync('dist/cjs/package.json', '{\\\"type\\\": \\\"commonjs\\\"}', 'utf8')\"",
    "build": "npm run build:esm && npm run build:cjs",
    "eval": "ts-node src/eval/index.ts",
    "eval:quick": "ts-node src/eval/index.ts run --quick --concurrency 4",
    "eval:custom": "ts-node src/eval/index.ts run --config",
    "eval:list": "ts-node src/eval/index.ts list",
    "eval:build": "npm run build && node dist/eval/index.js",
    "test": "vitest run",
    "docs": "typedoc",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agent",
    "llm",
    "sdk"
  ],
  "author": "qckfx",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@types/uuid": "^10.0.0",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "dotenv": "^16.0.3",
    "e2b": "^1.2.5",
    "glob": "^11.0.1",
    "ora": "^7.0.1",
    "prompts": "^2.4.2",
    "uuid": "^11.1.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/prompts": "^2.4.9",
    "typedoc": "^0.28.2",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
